<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="UI Group Suite" verbose="1" parallel="methods" thread-count="2">
    <test name="UI Tests Only">
        <groups>
            <run>
                <include name="ui"/>
            </run>
        </groups>
        <packages>
            <package name="ui.tests"/>
        </packages>
    </test>
</suite>

<?xml version="1.0" encoding="UTF-8"?>
<commonPage>
    <projectRow>//tbody/tr</projectRow>
    <createNewProject>//span[contains(text(), "Create")]/ancestor::button</createNewProject>
    <createProject>
        <name>//input[@id="project-name"]</name>
        <code>//input[@id="project-code"]</code>
        <create>//span[contains(text(), "Create project")]/ancestor::button</create>
        <accessType>//input[@type="radio" and @value="{value}"]</accessType>
    </createProject>
    <projectsBtn>//a[@href="/projects"]</projectsBtn>
    <openActionMenu>//a[contains(text(), "{name}")]/ancestor::tr//button[@aria-label="Open action menu"]</openActionMenu>
    <remove>//div[@data-testid="remove"]</remove>
    <okForDelete>//span[contains(text(), "Delete project")]/ancestor::button</okForDelete>
    <alertTxt>//div[@role="alert"]/descendant::span</alertTxt>
    <closeAlert>//button[@aria-label="Close"]</closeAlert>
</commonPage>
